{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"module": "commonjs",
		"noImplicitAny": false,
		"outDir": "./dist/",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"target": "ES5"
	},
	"include": ["./src/**/*", "test.js"]
}
